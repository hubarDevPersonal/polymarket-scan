{
  "name": "polymarket-orderbook-scanner",
  "version": "1.0.0",
  "description": "Real-time orderbook scanner for Polymarket crypto prediction markets with historical analytics",
  "author": "Artem Gubar",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/polymarket-scanner.git"
  },
  "keywords": [
    "polymarket",
    "prediction-markets",
    "orderbook",
    "crypto",
    "scanner",
    "clickhouse",
    "binance"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "tsx watch --env-file=.env src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.997.0",
    "@aws-sdk/lib-dynamodb": "^3.997.0",
    "@clickhouse/client": "^1.17.0",
    "@notionhq/client": "^5.9.0",
    "cors": "^2.8.6",
    "decimal.js": "^10.4.3",
    "express": "^4.21.0",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.0",
    "@types/node": "^22.0.0",
    "@types/ws": "^8.5.0",
    "eslint": "^10.0.2",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.8.1",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0",
    "typescript-eslint": "^8.56.1"
  }
}
